{
  "name": "To-Do",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "yarn",
    "start": "react-app-rewired start",
    "build": "env-cmd -f .env.${_ENVIRONMENT} react-app-rewired build",
    "test": "jest --logHeapUsage -c jest.config.js --maxWorkers=3",
    "test:integration": "jest -c jest.config.integration.js --maxWorkers=3",
    "test:reset": "rm -rf node_modules yarn.lock package-lock.json coverage && yarn",
    "e2e:dev": "TZ=UTC cypress open",
    "e2e": "TZ=UTC cypress run --headless",
    "lint": "eslint",
    "format": "prettier --write './(src|cypress)/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.7",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/pouchdb": "^6.4.0",
    "axios": "^0.21.3",
    "pouchdb": "^7.2.2",
    "qs": "^6.10.2",
    "react": "^17.0.1",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.22.5",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1",
    "rxjs": "^7.5.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/dom": "^8.7.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@tsconfig/node12": "^1.0.9",
    "@types/css-mediaquery": "^0.1.1",
    "@types/jest": "^27.0.2",
    "@types/jest-when": "^2.7.3",
    "@types/js-cookie": "^3.0.0",
    "@types/node": "^16.11.6",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "css-mediaquery": "^0.1.2",
    "cypress": "^8.5.0",
    "cypress-file-upload": "^5.0.8",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-dom": "^4.0.0",
    "jest-when": "^3.5.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "react-app-rewire-alias": "^1.1.3",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^5.0.0",
    "redux-devtools-extension": "^2.13.8",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^27.1.2",
    "typescript": "4.4.4",
    "yarn": "^1.22.15"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
